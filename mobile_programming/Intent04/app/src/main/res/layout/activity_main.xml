<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="전화걸기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="46dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/phone_no"/>
    <TextView
            android:text="전화번호"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/phone_no" android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/phone_no"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="252dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toEndOf="@+id/textView2"/>
    <Button
            android:text="전화걸기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/call_btn"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="49dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_no" app:layout_constraintEnd_toEndOf="parent"/>
</android.support.constraint.ConstraintLayout>