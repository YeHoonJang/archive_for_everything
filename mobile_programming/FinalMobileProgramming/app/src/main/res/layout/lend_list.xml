<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:id="@+id/constraintLayout">


    <TextView
            android:text="대여 내역"
            android:layout_width="match_parent"
            tools:layout_editor_absoluteX="129dp" android:id="@+id/textView4" android:layout_height="0dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/listView" android:textStyle="bold"
            android:textAlignment="textStart"/>
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        android:divider="#a2acef"
        android:dividerHeight="2dp"
        android:listSelector="#e4e5ed"
        android:id="@+id/listView" tools:layout_editor_absoluteY="64dp"/>
</android.support.constraint.ConstraintLayout>
